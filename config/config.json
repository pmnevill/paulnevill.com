{
  "mongo": {
    "srv": true,
    "url": "mymongodb-rucw6.mongodb.net/test?retryWrites=true"
  },
  "middleware": {
    "authn": {
      "priority": 39,
      "enabled": false,
      "route": "((?!api)(?!login)(?!callback)(?!logout))*",
      "module": {
        "name": "path:./lib/middleware/secured",
        "method": "authenticated"
      }
    },
    "appsec": {
      "module": {
        "arguments": [
          {
            "xframe": "SAMEORIGIN",
            "p3p": false,
            "csp": false
          }
        ]
      }
    },
    "router": {
      "module": {
        "arguments": [
          {
            "directory": "path:./controllers"
          }
        ]
      }
    }
  }
}
